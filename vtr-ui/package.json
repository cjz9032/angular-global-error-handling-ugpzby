{
	"name": "vtr-ui",
	"version": "1.11.0",
	"scripts": {
		"postinstall": "ngcc",
		"format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
		"format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
		"build:dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=dev",
		"build:dev1": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
		"build:dev2": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=dev  && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
		"build:beta": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=beta",
		"build:qa": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=qa",
		"build:pentest": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=pentest",
		"build:stage": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=stage",
		"build:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod",
		"start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
		"build-serve": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --prod",
		"build": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng build --prod",
		"serve": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve",
		"release": "standard-version",
		"mock-serve": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve -c=mock --watch",
		"ng": "ng",
		"test": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
		"lint": "ng lint",
		"lint:ci": "ng lint --tslintConfig=tslint-ci.json",
		"template-lint": "node prebuild/template-lint.js",
		"fix-no-console": "node prebuild/no-console.js",
		"update-nuspec": "node postbuild/nuget.js",
		"e2e": "ng e2e",
		"remove-console": "npx jscodeshift -t prebuild/removeConsole.js --extensions=ts src --ignore-config=./prebuild/jscodeshiftIgnore",
		"detect-console": "node prebuild/detectConsole.js",
		"diff": "node tools/code-diff.js",
		"get-https": "node tools/collect-https.js"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~11.2.3",
		"@angular/cdk": "^11.2.2",
		"@angular/common": "~11.2.3",
		"@angular/core": "~11.2.3",
		"@angular/forms": "~11.2.3",
		"@angular/localize": "^11.2.3",
		"@angular/platform-browser": "~11.2.3",
		"@angular/platform-browser-dynamic": "~11.2.3",
		"@angular/router": "~11.2.3",
		"@angular/service-worker": "~11.2.3",
		"@fortawesome/angular-fontawesome": "^0.8.2",
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-regular-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@fortawesome/pro-light-svg-icons": "^5.15.2",
		"@lenovo/material": "^11.2.2",
		"@lenovo/tan-client-bridge": "10.2103.0.unstablelatest",
		"@ng-bootstrap/ng-bootstrap": "^7.0.0",
		"@ngx-translate/core": "^13.0.0",
		"@ngx-translate/http-loader": "^6.0.0",
		"angular-svg-icon": "^11.2.0",
		"angular2-uuid": "^1.1.1",
		"bootstrap": "^4.6.0",
		"core-js": "^3.9.1",
		"crypto-js": "^4.0.0",
		"currency-formatter": "^1.5.6",
		"d3-selection": "^2.0.0",
		"fuse.js": "^6.4.6",
		"hammerjs": "^2.0.8",
		"inversify": "^5.0.5",
		"jspdf": "^2.3.0",
		"jspdf-autotable": "^3.5.14",
		"localforage": "^1.9.0",
		"lodash": "^4.17.21",
		"mitt": "~2.1.0",
		"moment": "^2.29.1",
		"ng5-slider": "1.1.14",
		"ngforage": "^6.0.0",
		"ngx-bootstrap": "^6.2.0",
		"ngx-clipboard": "^14.0.1",
		"ngx-color-picker": "^11.0.0",
		"ngx-cookie-service": "^11.0.2",
		"reinvented-color-wheel": "^0.3.0",
		"rxjs": "^6.6.6",
		"safe-pipe": "^1.0.3",
		"ts-md5": "^1.2.7",
		"tslib": "^2.1.0",
		"x2js": "^3.4.0",
		"zone.js": "^0.10.3"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^11.1.0",
		"@angular-devkit/build-angular": "~0.1102.2",
		"@angular-eslint/builder": "1.2.0",
		"@angular-eslint/eslint-plugin": "1.2.0",
		"@angular-eslint/eslint-plugin-template": "1.2.0",
		"@angular-eslint/schematics": "^1.2.0",
		"@angular-eslint/template-parser": "1.2.0",
		"@angular/cli": "~11.2.2",
		"@angular/compiler": "^11.2.3",
		"@angular/compiler-cli": "~11.2.3",
		"@angular/language-service": "~11.2.3",
		"@fortawesome/fontawesome-pro": "^5.15.2",
		"@types/d3-selection": "^2.0.0",
		"@types/jasmine": "~3.6.4",
		"@types/jasminewd2": "^2.0.8",
		"@types/lodash": "^4.14.168",
		"@types/node": "^14.14.31",
		"@typescript-eslint/eslint-plugin": "4.15.2",
		"@typescript-eslint/parser": "4.15.2",
		"acorn": "^8.0.5",
		"eslint": "^7.21.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "32.2.0",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"glob": "^7.1.6",
		"html-loader": "^1.3.2",
		"htmlparser2": "^6.0.0",
		"husky": "^4.3.8",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"jscodeshift": "^0.11.0",
		"karma": "~6.1.1",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"karma-junit-reporter": "^2.0.1",
		"ng-packagr": "^11.2.4",
		"prettier": "^2.2.1",
		"protractor": "~7.0.0",
		"recursive-readdir": "^2.2.2",
		"simple-git": "^2.35.2",
		"standard-version": "^9.1.1",
		"ts-node": "^9.1.1",
		"tslint": "~6.1.3",
		"typescript": "~4.1.3",
		"webpack": "webpack-4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run detect-console"
		}
	}
}
