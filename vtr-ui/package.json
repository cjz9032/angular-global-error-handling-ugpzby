    "jscodeshift": "^0.12.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^11.2.4",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "recursive-readdir": "^2.2.2",
    "simple-git": "^2.38.0",
    "standard-version": "^9.2.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5",
    "webpack": "webpack-4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run detect-console"
    }
  }
}
