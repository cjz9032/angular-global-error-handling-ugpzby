{
  "name": "vtr-ui",
  "version": "1.7.16",
  "scripts": {
    "build:dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=dev",
    "build:dev1": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
    "build:dev2": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=dev  && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
    "build:beta": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=mock",
    "build:qa": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=qa",
    "build:stage": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=stage",
    "build:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod",
    "ng": "ng",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
    "build-serve": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --prod",
    "build": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng build --prod",
    "serve": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve",
    "test": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadless --codeCoverage=true",
    "lint": "ng lint",
    "lint:ci": "ng lint --tslintConfig=tslint-ci.json",
    "template-lint": "node prebuild/template-lint.js",
    "fix-no-console": "node prebuild/no-console.js",
    "update-nuspec": "node postbuild/nuget.js",
    "e2e": "ng e2e",
    "release": "standard-version",
    "dev++": "npm run release && git push --follow-tags origin develop",
    "pwa": "http-server -p 4200 ./dist/vtr-ui",
    "wba": "ng build --prod --stats-json & webpack-bundle-analyzer dist/vtr-ui/stats.json",
    "sme": "ng build & source-map-explorer 'dist/vtr-ui/*.js'",
    "mock-serve": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve -c=mock --watch",
    "remove-console": "npx jscodeshift -t prebuild/removeConsole.js --extensions=ts --parser=ts src --ignore-config=./prebuild/jscodeshiftIgnore",
    "detect-console": "node prebuild/detectConsole.js",
    "diff": "node tools/code-diff.js",
    "get-https": "node tools/collect-https.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/common": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/localize": "^10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@angular/service-worker": "~10.0.4",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/pro-light-svg-icons": "^5.14.0",
    "@lenovo/tan-client-bridge": "10.2009.0.unstablelatest",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-svg-icon": "^10.0.0",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "d3-selection": "^1.4.1",
    "hammerjs": "^2.0.8",
    "inversify": "^5.0.1",
    "lodash": "^4.17.19",
    "mitt": "~2.1.0",
    "moment": "^2.27.0",
    "ng5-slider": "1.1.14",
    "ngx-bootstrap": "^5.6.1",
    "ngx-clipboard": "^13.0.1",
    "ngx-color-picker": "^9.1.0",
    "ngx-cookie-service": "^3.0.4",
    "reinvented-color-wheel": "^0.2.9",
    "rxjs": "^6.6.0",
    "safe-pipe": "^1.0.3",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "x2js": "^3.4.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "next",
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/build-ng-packagr": "~0.1000.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler": "^10.0.4",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@fortawesome/fontawesome-pro": "^5.14.0",
    "@lenovo/codelyzer": "6.0.2",
    "@types/d3-selection": "^1.4.2",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.24",
    "acorn": "^7.3.1",
    "glob": "^7.1.6",
    "html-loader": "^1.1.0",
    "htmlparser2": "^4.1.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jscodeshift": "^0.10.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^10.0.2",
    "protractor": "~7.0.0",
    "recursive-readdir": "^2.2.2",
    "simple-git": "^2.14.0",
    "standard-version": "^8.0.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run detect-console"
    }
  }
}
