{
	"name": "vtr-ui",
	"version": "1.0.267",
	"scripts": {
		"build:dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=dev",
		"build:qa": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=qa",
		"build:stage": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build -c=stage",
		"build:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod",
		"ng": "ng",
		"start": "ng serve",
		"build-serve": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --prod",
		"build": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build --prod",
		"serve": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve",
		"test": "ng test",
		"lint": "ng lint",
		"fix-no-console": "node prebuild/no-console.js",
		"update-nuspec": "node postbuild/nuget.js",
		"e2e": "ng e2e",
		"pwa": "ng build --prod & angular-http-server --path ./dist/vtr-ui -p 4200 --silent -c-1 ",
		"pwa2": "http-server -p 4200 ./dist/vtr-ui -c-1 ",
		"release": "standard-version",
		"dev++": "npm run release && git push --follow-tags origin develop",
		"stats": "ng build --prod --stats-json",
		"analyze": "webpack-bundle-analyzer dist/vtr-ui/stats.json",
		"dev-stats": "ng build -c=dev --stats-json & webpack-bundle-analyzer dist/vtr-ui/stats.json",
		"source": "ng build -c=dev & source-map-explorer 'dist/vtr-ui/*.js'",
		"mock-serve": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng serve --port 4300 -c=mock"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.1.0",
		"@angular/common": "~8.1.0",
		"@angular/compiler": "~8.1.0",
		"@angular/core": "~8.1.0",
		"@angular/forms": "~8.1.0",
		"@angular/platform-browser": "~8.1.0",
		"@angular/platform-browser-dynamic": "~8.1.0",
		"@angular/pwa": "^0.803.4",
		"@angular/router": "~8.1.0",
		"@angular/service-worker": "~8.1.0",
		"@fortawesome/angular-fontawesome": "^0.4.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.17",
		"@fortawesome/free-brands-svg-icons": "^5.9.0",
		"@fortawesome/free-regular-svg-icons": "^5.9.0",
		"@fortawesome/free-solid-svg-icons": "^5.9.0",
		"@fortawesome/pro-light-svg-icons": "^5.9.0",
		"@lenovo/tan-client-bridge": "^10.1912.0-alpha.2",
		"@ng-bootstrap/ng-bootstrap": "^5.1.0",
		"@ngx-translate/core": "^11.0.1",
		"@ngx-translate/http-loader": "^4.0.0",
		"angular-svg-icon": "^7.2.0",
		"angular2-uuid": "^1.1.1",
		"bootstrap": "^4.3.1",
		"core-js": "^2.5.4",
		"d3": "^5.9.2",
		"inversify": "^5.0.1",
		"js-sha1": "^0.6.0",
		"js-sha256": "^0.9.0",
		"lodash": "^4.17.15",
		"mitt": "~1.2.0",
		"ng5-slider": "1.1.14",
		"ngx-color-picker": "^8.0.0",
		"ngx-cookie-service": "^2.1.0",
		"popper.js": "^1.15.0",
		"reinvented-color-wheel": "^0.2.8",
		"rxjs": "^6.5.2",
		"tslib": "^1.9.0",
		"x2js": "^3.2.6",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^8.0.2",
		"@angular-devkit/build-angular": "~0.801.0",
		"@angular/cli": "~8.1.0",
		"@angular/compiler-cli": "~8.1.0",
		"@angular/language-service": "~8.1.0",
		"@fortawesome/fontawesome-pro": "^5.8.1",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"acorn": "^6.0.5",
		"codelyzer": "^5.0.0",
		"html-loader": "^0.5.5",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"protractor": "~5.4.0",
		"standard-version": "^5.0.2",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.4.3"
	}
}
