{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/*.woff",
					"/*.css",
					"/assets/cms-cache/**",
					"/assets/images/keyboard-images/KeyboardMap_Images/**"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**",
					"!/assets/i18n/**"
				],
				"urls": [
					"https://vantage*.csw.lenovo.com/**/*.jpg",
					"https://vantage*.csw.lenovo.com/**/*.png",
					"https://vantage*.csw.lenovo.com/**/*.svg",
					"https://vantage*.csw.lenovo.com/**/*.webp"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "cms",
			"urls": [
				"https://vantage*.csw.lenovo.com/api/**",
				"https://vantage*.csw.lenovo.com/*.js"
			],
			"cacheConfig": {
				"maxSize": 50,
				"maxAge": "24h",
				"timeout": "5s",
				"strategy": "performance"
			}
		}
	]
}