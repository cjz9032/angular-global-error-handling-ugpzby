{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/*.woff",
					"/index.html",
					"/*.css",
					"/assets/cms-cache/**"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"!/assets/i18n/**",
					"!/assets/privacy-json/trackers.json",
					"/assets/**"
				],
				"urls": [
					"https://qa.csw.lenovo.com/**",
					"https://vantage-qa.csw.lenovo.com/**",
					"https://vantage.csw.lenovo.com/**",
					"https://cms.csw.lenovo.com/**"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "cms",
			"urls": [
				"https://vantage-qa.csw.lenovo.com/**",
				"https://vantage.csw.lenovo.com/**",
				"https://cms.csw.lenovo.com/**"
			],
			"cacheConfig": {
				"maxSize": 50,
				"maxAge": "24h",
				"timeout": "5s",
				"strategy": "freshness"
			}
		}
	]
}
