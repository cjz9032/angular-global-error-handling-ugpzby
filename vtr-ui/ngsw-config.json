{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/*.woff",
					"/*.css",
					"/assets/cms-cache/**",
					"/assets/images/keyboard-images/KeyboardMap_Images/**"
				],
				"urls": [
					"/**/*.js"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**",
					"!/assets/i18n/**"
				],
				"urls": [
					"/**/*.jpg",
					"/**/*.png",
					"/**/*.svg",
					"/**/*.webp"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "cms",
			"urls": [
				"/**/*api*/**"
			],
			"cacheConfig": {
				"maxSize": 50,
				"maxAge": "24h",
				"timeout": "5s",
				"strategy": "performance"
			}
		}
	]
}