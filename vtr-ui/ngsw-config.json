{
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/favicon.ico",
					"/index.html",
					"/*.css"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "prefetch",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**"
				],
				"urls": [
					"https://qa.csw.lenovo.com/**",
					"https://vantage-qa.csw.lenovo.com/**",
					"https://vantage.csw.lenovo.com/**",
					"https://cms.csw.lenovo.com/**"
				]
			}
		}
	],
	"dataGroups": [
		{
			"name": "cms",
			"urls": [
				"https://vantage-qa.csw.lenovo.com/**",
				"https://vantage.csw.lenovo.com/**"
			],
			"cacheConfig": {
				"maxSize": 10,
				"maxAge": "24h",
				"timeout": "5s",
				"strategy": "freshness"
			}
		}
	]
}
