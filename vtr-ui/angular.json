{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"vtr-ui": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "vtr",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"architect": {
				"build": {
					"builder": "@angular-builders/custom-webpack:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"rxjs",
							"lodash",
							"inversify",
							"uuid",
							"crypto-js",
							"hammerjs",
							"util",
							"jspdf-autotable"
						],
						"customWebpackConfig": {
							"path": "./custom-webpack.config.js"
						},
						"outputPath": "dist/vtr-ui",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": true,
						"assets": [
							"src/favicon.ico",
							"src/assets",
							"src/manifest.webmanifest",
							"src/pre-load.js"
						],
						"styles": ["src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": true,
							"aot": true,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "10mb",
									"maximumError": "15mb"
								}
							],
							"serviceWorker": true
						},
						"dev": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.dev.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								}
							],
							"optimization": false,
							"sourceMap": true,
							"namedChunks": true,
							"aot": true,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": true,
							"serviceWorker": false
						},
						"qa": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.qa.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": {
								"scripts": true,
								"styles": true,
								"vendor": true
							},
							"namedChunks": true,
							"aot": true,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "10mb",
									"maximumError": "15mb"
								}
							],
							"serviceWorker": true
						},
						"stage": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.stage.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": true,
							"aot": true,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "10mb",
									"maximumError": "15mb"
								}
							],
							"serviceWorker": true
						},
						"mock": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.mock.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								},
								{
									"replace": "src/app/services/vantage-shell/vantage-shell.service.ts",
									"with": "src/app/services/vantage-shell/vantage-shell-mock.service.ts"
								}
							],
							"optimization": false,
							"outputHashing": "none",
							"sourceMap": true,
							"namedChunks": false,
							"aot": false,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "10mb",
									"maximumError": "15mb"
								}
							],
							"serviceWorker": false
						},
						"beta": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.beta.ts"
								},
								{
									"replace": "src/app/services/mock/mock.service.ts",
									"with": "src/app/services/mock/mock.service.prod.ts"
								},
								{
									"replace": "src/app/services/security/securityMock.service.ts",
									"with": "src/app/services/security/securityMock.service.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": true,
							"aot": true,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "10mb",
									"maximumError": "15mb"
								}
							],
							"serviceWorker": true
						}
					}
				},
				"serve": {
					"builder": "@angular-builders/custom-webpack:dev-server",
					"options": {
						"browserTarget": "vtr-ui:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "vtr-ui:build:production"
						},
						"mock": {
							"browserTarget": "vtr-ui:build:mock"
						},
						"stage": {
							"browserTarget": "vtr-ui:build:stage"
						},
						"qa": {
							"browserTarget": "vtr-ui:build:qa"
						},
						"beta": {
							"browserTarget": "vtr-ui:build:beta"
						},
						"dev": {
							"browserTarget": "vtr-ui:build:dev"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "vtr-ui:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"styles": ["src/styles.scss"],
						"scripts": [],
						"assets": ["src/favicon.ico", "src/assets", "src/manifest.webmanifest"],
						"codeCoverage": true,
						"fileReplacements": [
							{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.mock.ts"
							},
							{
								"replace": "src/app/services/mock/mock.service.ts",
								"with": "src/app/services/mock/mock.service.prod.ts"
							},
							{
								"replace": "src/app/services/vantage-shell/vantage-shell.service.ts",
								"with": "src/app/services/vantage-shell/vantage-shell-mock.service.ts"
							}
						]
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "vtr-ui:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "vtr-ui:serve:production"
						}
					}
				}
			}
		}
	},
	"defaultProject": "vtr-ui",
	"cli": {
		"analytics": false
	}
}
